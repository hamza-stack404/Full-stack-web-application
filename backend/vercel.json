{
  "builds": [
    {
      "src": "serverless/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "serverless/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
